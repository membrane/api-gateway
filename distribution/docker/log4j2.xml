<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{ABSOLUTE} %5p %tid %tn %c{1}:%L - %m%n" />
        </Console>
    </Appenders>
    <Loggers>
<!--        <Logger name="com.predic8.membrane.core.openapi" level="debug">-->
<!--            <AppenderRef ref="STDOUT" />-->
<!--        </Logger>-->
        <Root level="info">
            <AppenderRef ref="STDOUT" />
        </Root>
    </Loggers>
</Configuration>
